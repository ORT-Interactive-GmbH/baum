{
  "name": "baum/baum",
  "type": "library",
  "description": "Baum is an implementation of the Nested Set pattern for Eloquent models.",
  "keywords": ["nested set", "laravel", "laravel 4", "laravel 5", "eloquent", "database"],
  "license": "MIT",
  "authors": [
    {
      "name": "Estanislau Trepat",
      "email": "estanis@etrepat.com",
      "homepage": "http://etrepat.com"
    }
  ],
  "require": {
    "php": ">=7.3",
    "illuminate/console": "^6.0|^7.0",
    "illuminate/database": "^6.0|^7.0",
    "illuminate/events": "^6.0|^7.0",
    "illuminate/filesystem": "^6.0|^7.0",
    "illuminate/support": "^6.0|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.0",
    "mockery/mockery": "~1.3",
    "d11wtq/boris": "~1.0.10"
  },
  "autoload": {
    "psr-4": {
      "": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Baum\\Tests\\": "tests/"
    }
  }
}
